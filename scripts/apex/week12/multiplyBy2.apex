//write a program that can multiply each number by 2
//ex: list = [1,2,3,4,5];

//for loop

List<Integer> list1=new List<Integer> {1,2,3,4,5};

for(Integer i=0;i<list1.size();i++){

        list1[i]*=2;

    }

System.debug(list1); 

// for each loop !!!!

for(Integer each : list1) {
      
        each*=2;
    
}

System.debug(list1); 









        
        




    
